.produto-imagem-miniaturas
    display: none 

.thumbs
    @media desktop
        order: -1
        width 95px
    
    .slick
        &-prev
            @media desktop
                top: 0
                transform: rotate(90deg);

            @media mobile 
                left -15px
                top: 60%

        &-next 
            @media desktop
                top: 100% 
                transform: rotate(90deg);
                left 18px

            @media mobile 
                right -15px
                top: 60%

            
    &__list 
        padding: 70px 0 30px;


    &__item 
        margin: 5px 0
        img 
            width: 80px
            height: 80px 
            object-fit: contain 
            @media mobile 
                margin: 0 auto

    &__link 
        display: block
        .thumb
            &__video
                &:before
                    content: ''
                    background: url('data:image/png;base64,')no-repeat
                    display: block
                    width: 80px
                    height 80px
                    margin: 0 auto


.produto-video 
    background: white
    bottom: 0
    left: 50%
    position: absolute
    right: 0
    top: 0
    z-index: 9999
    transform: translateX(-50%)
    width: 74%
    height 630px
    @media mobile 
        height: 67%
        width: 100%
    .bloco 
        display: block
        height: 100% !important
        width: 100% !important
        border: none !important
        background: white
        >iframe
            height: 100%
            width: 100%
